{
	"private": true,
	"scripts": {
		"precommit": "npm -s run test",
		"start": "node dev/build.js && parcel dev/index.html",
		"lint": "eslint --ext .json . && npm -s run lint:i18n && npm -s run lint:js && npm run lint:css",
		"lint:js": "eslint --cache --max-warnings 0 --report-unused-disable-directives .",
		"lint:css": "stylelint '**/*.less'",
		"lint:i18n": "banana-checker --requireLowerCase=0 i18n/",
		"test": "npm -s run lint",
		"jsdoc": "jsdoc -c jsdoc.json",
		"doc": "npm run jsdoc"
	},
	"engines": {
		"node": "10.15.2"
	},
	"pre-commit": "precommit",
	"alias": {
		"vue": "./dev/vue.js"
	},
	"devDependencies": {
		"@vue/component-compiler-utils": "^3.1.1",
		"eslint-config-wikimedia": "0.15.0",
		"grunt-banana-checker": "0.9.0",
		"jsdoc": "3.6.3",
		"less": "3.11.1",
		"mustache": "3.0.1",
		"parcel-bundler": "1.12.4",
		"pre-commit": "1.2.2",
		"stylelint": "10.0.1",
		"stylelint-config-wikimedia": "0.7.0",
		"vue": "2.6.11",
		"vue-hot-reload-api": "2.3.4",
		"vue-template-compiler": "2.6.11"
	}
}
